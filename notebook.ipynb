   "source": [
    "import tkinter as tk\n",
    "\n",
    "# Créer la fenêtre principale\n",
    "root = tk.Tk()\n",
    "\n",
    "# Ajouter un label\n",
    "label = tk.Label(root, text=\"Bonjour, Tkinter dans Jupyter!\")\n",
    "label.pack()\n",
    "\n",
    "# Lancer la boucle principale\n",
    "root.mainloop()\n"
   ]
